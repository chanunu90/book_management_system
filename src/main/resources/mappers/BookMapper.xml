<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kolis3.book_management_system.mappers.BookMapper">

  <select id="time">
    select now()
  </select>


  <select id="getMamber">
    select * 
    from tbl_book
    <where>
        bno > 0
    </where>
    limit #{skip} , #{size}
  </select>


</mapper>